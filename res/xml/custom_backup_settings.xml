<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="Backup and Restore Settings"
        >

    <PreferenceCategory
		android:title="B A C K U P   A N D   R E S T O R E"
		android:key="junk_backup_restore"
		>    
    

    <Preference
		android:layout="?android:attr/preferenceLayoutChild"
		android:key="junk_backup"
		android:title="Backup Junk Settings" 
		android:summary="Backup Junk settings and themes"
		/>       

    <Preference
		android:layout="?android:attr/preferenceLayoutChild"
		android:key="junk_restore"
		android:title="Restore Junk Settings" 
		android:summary="Restore Junk settings and themes"
		/>        
 
  	
	</PreferenceCategory>    


</PreferenceScreen>
